{
  "name": "mais-precoce-ampl",
  "version": "0.9.0",
  "main": "dist/ampl.umd.js",
  "module": "dist/ampl.esm.js",
  "unpkg": "dist/ampl.min.js",
  "browser": {
    "./sfc": "src/component/FormNewOptimization.vue"
  },
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:coverage": "npm run serve --mode test",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/ampl.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/ampl.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/ampl.min.js",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "deploy": "node scripts/gh-pages-deploy.js",
    "test:component": "vue-cli-service test:unit ./tests/component/*.spec.js"
  },
  "dependencies": {
    "circular-dependency-plugin": "^5.2.2",
    "core-js": "^3.6.5",
    "mathjs": "^8.0.1",
    "vue": "^2.6.11",
    "vue-mathjs": "^0.1.1",
    "vuetify": "^2.2.11"
  },
  "devDependencies": {
    "@mdi/font": "^5.8.55",
    "@mdi/js": "^5.8.55",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-nightwatch": "^4.5.9",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-mocha": "^4.5.9",
    "@vue/cli-service": "~4.5.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chromedriver": "86",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "execa": "^5.0.0",
    "geckodriver": "^1.20.0",
    "mocha": "^8.2.1",
    "rollup": "^2.30.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-vue": "^5.0.1",
    "rollup-plugin-vuetify": "^0.2.4",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0",
    "vue-cli-plugin-vuetify": "~2.0.7",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/**/*.spec.{j,t}s?(x)",
          "dist/*",
          "src/*",
          "public/*",
          "*.json",
          "*.js"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
